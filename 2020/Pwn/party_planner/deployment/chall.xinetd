service chall
{
        disable         = no
        socket_type     = stream
        protocol        = tcp
        user            = ctf
        wait            = no
        type            = UNLISTED
        bind            = 0.0.0.0
        port            = 1337
        server          = /home/ctf/run.sh
        instances       = 50
        log_type        = FILE /var/log/xinetd
        log_on_success  = HOST PID
        log_on_failure  = HOST
        rlimit_cpu      = 60
}

